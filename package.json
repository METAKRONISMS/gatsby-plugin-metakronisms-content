{
  "private": "true",
  "name": "gatsby-plugin-metakronisms-content",
  "main": "gatsby-node",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/METAKRONISMS/gatsby-plugin-metakronisms-content.git"
  },
  "keywords": [
    "METAKRONISMS",
    "gatsby",
    "plugin",
    "content"
  ],
  "author": "Valentin Vago <zeropaper@irata.ch>",
  "engines": {
    "npm": "^6.14.4",
    "node": "12.8"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/METAKRONISMS/gatsby-plugin-metakronisms-content/issues"
  },
  "homepage": "https://github.com/METAKRONISMS/gatsby-plugin-metakronisms-content#readme",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "lint": "eslint --cache --format codeframe --ext mjs,jsx,js src gatsby-node.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "local": "npm-run-all -p local:build local:gatsby",
    "local:build": "nodemon -w .neutrinorc.js -w src -e js,jsx -x 'BUILD_SKIP_MIN=1 npm run build && npm run local:build:plugin && npm run local:build:demo'",
    "local:build:plugin": "cp -rf dist ../metakronisms/node_modules/gatsby-plugin-metakronisms-content/",
    "local:build:demo": "cp -rf dist ../metakronisms-demo/node_modules/gatsby-plugin-metakronisms-content/",
    "local:gatsby": "nodemon -w gatsby-node.js -x 'npm run local:gatsby:plugin && npm run local:gatsby:demo'",
    "local:gatsby:plugin": "cp -f gatsby-node.js ../metakronisms/node_modules/gatsby-plugin-metakronisms-content/",
    "local:gatsby:demo": "cp -f gatsby-node.js ../metakronisms-demo/node_modules/gatsby-plugin-metakronisms-content/"
  },
  "peerDependencies": {
    "core-js": "^3.6.5",
    "culori": "^0.17.0",
    "gatsby-plugin-mdx": "^1.2.36",
    "gatsby-plugin-transition-link": "^1.20.2",
    "gsap": "^3.5.1",
    "@mdx-js/react": "^1.6.16",
    "d3": "^5.16.0",
    "prop-types": "^15.7.2",
    "react-jss": "^10.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@mdx-js/react": "^1.6.16",
    "@neutrinojs/eslint": "^9.3.0",
    "@neutrinojs/react-components": "^9.3.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "culori": "^0.17.0",
    "d3": "^5.16.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby-plugin-mdx": "^1.2.36",
    "gatsby-plugin-transition-link": "^1.20.2",
    "gsap": "^3.5.1",
    "neutrino": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-jss": "^10.4.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {}
}
